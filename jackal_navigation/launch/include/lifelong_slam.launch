<launch>
  <arg name="scan_topic" default="$(eval optenv('JACKAL_LASER_TOPIC', 'front/scan'))" />

  <node pkg="slam_toolbox" type="lifelong_slam_toolbox_node" name="slam_toolbox" output="screen">
    <rosparam command="load" file="$(find jackal_navigation)/params/lifelong_mapping.yaml" />
    <param name="scan_topic" value="/$(arg scan_topic)"/>
  </node>
</launch>
